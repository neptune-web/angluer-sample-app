{
    "version": "1.5.3",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true
    },
    "files": [
        "./app/app.ts",
        "./app/components/about/about.ts",
        "./app/components/about/about_spec.ts",
        "./app/components/home/home.ts",
        "./app/components/home/home_spec.ts",
        "./app/components/users/services/interfaces.ts",
        "./app/components/users/services/users-service.ts",
        "./app/components/users/user-details/user-details.ts",
        "./app/components/users/user-form/user-form.ts",
        "./app/components/users/users-home/users-home.ts",
        "./app/components/users/users-list/users-list.ts",
        "./app/components/users/users.ts",
        "./app/directives/loading-button.ts",
        "./app/directives/set-active.ts",
        "./app/init.ts",
        "./app/services/NameList.ts",
        "./app/services/NameList_spec.ts",
        "./app/typings/_custom.d.ts",
        "./app/typings/ng2_test.d.ts",
        "./app/typings/systemjs.d.ts",
        "./tsd_typings/jasmine/jasmine.d.ts",
        "./tsd_typings/systemjs/systemjs.d.ts",
        "./tsd_typings/tsd.d.ts"
    ],
    "filesGlob": [
        "./app/**/*.ts",
        "./tsd_typings/**/*.d.ts"
    ],
    "compileOnSave": false
}
